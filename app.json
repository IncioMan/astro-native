{
  "expo": {
    "name": "Astroport",
    "slug": "astro-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/astro-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/al-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000e37"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.incioman.astronative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
